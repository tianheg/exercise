#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/htmlnano@2.0.1_svgo@2.8.0/node_modules/htmlnano/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/htmlnano@2.0.1_svgo@2.8.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/svgo@2.8.0/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/htmlnano@2.0.1_svgo@2.8.0/node_modules/htmlnano/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/htmlnano@2.0.1_svgo@2.8.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
else
  exec node  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
fi
