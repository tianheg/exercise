#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules/acorn/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/acorn@8.7.0/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules/acorn/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
