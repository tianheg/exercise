#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node-gyp-build-optional-packages@4.3.1/node_modules/node-gyp-build-optional-packages/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node-gyp-build-optional-packages@4.3.1/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/msgpackr-extract@1.1.3/node_modules/msgpackr-extract/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/msgpackr-extract@1.1.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node-gyp-build-optional-packages@4.3.1/node_modules/node-gyp-build-optional-packages/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node-gyp-build-optional-packages@4.3.1/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules:/home/archie/repo/exercise/web/others/bootstrap/node_modules:/home/archie/repo/exercise/web/others/node_modules:/home/archie/repo/exercise/web/node_modules:/home/archie/repo/exercise/node_modules:/home/archie/repo/node_modules:/home/archie/node_modules:/home/node_modules:/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/msgpackr-extract@1.1.3/node_modules/msgpackr-extract/node_modules:/home/archie/repo/exercise/web/others/bootstrap/first-example/node_modules/.pnpm/msgpackr-extract@1.1.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp-build-optional-packages@4.3.1/node_modules/node-gyp-build-optional-packages/build-test.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp-build-optional-packages@4.3.1/node_modules/node-gyp-build-optional-packages/build-test.js" "$@"
fi
